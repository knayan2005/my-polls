{
  "name": "<%= _.slugify(appname) %>",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node app.js"
  },
  "dependencies": {
    "express": "~4.2.0",
    "glob": "^4.0.5",
    "serve-favicon": "~2.0.1",
    "morgan": "~1.1.1",
    "cookie-parser": "~1.3.2",
    "body-parser": "~1.4.3",
    "compression": "~1.0.8",
    "method-override": "~2.1.1",
    "glob": "~4.0.5"<% if(options.database == 'mongodb'){ %>,
    "mongoose": "~3.8.12"<% } %><% if(options.database == 'mysql'){ %>,
    "sequelize": "~1.7.9",
    "lodash": "~2.4.1",
    "mysql": "~2.4.0"<% } %><% if(options.database == 'postgresql'){ %>,
    "sequelize": "~1.7.9",
    "lodash": "~2.4.1",
    "pg": "~3.4.0"<% } %><% if(options.viewEngine == 'jade'){ %>,
    "jade": "~1.3.0"<% } %><% if(options.viewEngine == 'ejs'){ %>,
    "ejs": "~1.0.0"<% } %>
  },
  "devDependencies": {<% if(options.buildTool == 'grunt'){ %>
    "grunt": "~0.4.5",
    "grunt-develop": "~0.4.0"<% if(options.cssPreprocessor == 'sass'){ %>,
